{
  "name": "tsds-install",
  "version": "1.1.5",
  "description": "Install a package with retry",
  "keywords": [
    "dev",
    "development",
    "install",
    "npm",
    "typescript"
  ],
  "homepage": "https://github.com/kmalakoff/tsds-install",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/tsds-install.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "tsds format",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^2.0.0",
    "getopts-compat": "^2.2.6",
    "node-version-call": "^1.11.0",
    "queue-cb": "^1.0.0",
    "readable-stream": "^2.0.2",
    "tsds-lib": "^1.20.0"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "module-link-unlink": "^1.0.11",
    "node-version-use": "*",
    "os-shim": "^0.1.0",
    "resolve": "^1.3.1",
    "short-hash": "^1.0.0",
    "ts-dev-stack": "*",
    "tsds-config": "*",
    "tsds-lib-test": "^1.19.7"
  },
  "engines": {
    "node": ">=0.8"
  }
}
